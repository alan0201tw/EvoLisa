# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  EvoLisa_config = debug

else ifeq ($(config),release)
  EvoLisa_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := EvoLisa

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

EvoLisa:
ifneq (,$(EvoLisa_config))
	@echo "==== Building EvoLisa ($(EvoLisa_config)) ===="
	@${MAKE} --no-print-directory -C . -f EvoLisa.make config=$(EvoLisa_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f EvoLisa.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   EvoLisa"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"